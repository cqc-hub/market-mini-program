.goods-item {
  padding-bottom: 100rpx;
  
 
}
.imagecqc{
  width: 100%;
  height: 100%;
  border-radius: 5px;
}

.goods-info {
  font-size: 24rpx;
  position: absolute;
  bottom: 0rpx;
  left: 0;
  right: 0;
  overflow: hidden;
  text-align: center;
}
.title {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-bottom: 6rpx;
}

.goods-info .price {
  color: rgba(94, 19, 116, 0.97);
  margin-right: 40rpx;
}

.goods-info .collect {
  position: relative;
}

.goods-info .collect::before {
  content: '';
  position: absolute;
  left: -30rpx;
  top: 0;
  width: 28rpx;
  height: 28rpx;
   background: url("data:image/png;base64,") 0 0/14px 14px;
}